{
  "name": "simple-tjscli",
  "version": "0.11.2",
  "description": "Create JSONSchema from typescript interface using creation engine",
  "scripts": {
    "script": "cross-env DEBUG=tjscli:* TS_NODE_FILES=true TS_NODE_PROJECT=./tsconfig.prod.json ts-node ./src/tjscli.ts",
    "clean": "rimraf dist artifact",
    "prepub": "npm run build",
    "pub": "npm publish --registry https://registry.npmjs.org --access=public",
    "prepub:dev": "npm run build",
    "pub:dev": "npm publish --registry http://localhost:8901 --force",
    "build:tsc": "tsc --noemit -p tsconfig.json",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production npx webpack --config webpack.config.prod.js",
    "prebuild:dev": "npm run clean",
    "build:dev": "cross-env NODE_ENV=develop npx webpack --config webpack.config.dev.js",
    "lint": "tslint src/**/*.ts",
    "debug": "cross-env DEBUG=tjscli:* TS_NODE_PROJECT=tsconfig.json TS_NODE_FILES=true node -r ts-node/register -r tsconfig-paths/register --inspect --nolazy src/tjscli.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/tjscli.git"
  },
  "bin": {
    "tjscli": "dist/index.js"
  },
  "private": false,
  "author": "ByungJoon Lee",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/imjuni/tjscli/issues"
  },
  "homepage": "https://github.com/imjuni/tjscli#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/figlet": "^1.5.4",
    "@types/gulp": "^4.0.9",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^27.0.2",
    "@types/json5": "^2.2.0",
    "@types/luxon": "^2.0.5",
    "@types/node": "^16.7.1",
    "@types/prettier": "^2.4.1",
    "@types/yargs": "^17.0.4",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "docsify-cli": "^4.4.3",
    "jest": "^27.3.1",
    "maid": "0.3.0",
    "rimraf": "3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "debug": "^4.3.2",
    "fast-glob": "^3.2.7",
    "fp-ts": "^2.11.5",
    "fuzzy": "^0.1.3",
    "inquirer": "^8.2.0",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "json5": "^2.2.0",
    "luxon": "^2.0.2",
    "my-easy-fp": "^0.9.0",
    "prettier": "^2.5.1",
    "ts-json-schema-generator": "^0.96.0",
    "tslib": "^2.3.1",
    "typescript-json-schema": "^0.51.0",
    "uuid": "^8.3.2",
    "yargs": "^17.2.1"
  }
}
