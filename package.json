{
  "name": "simple-tjscli",
  "version": "0.11.3",
  "description": "Create JSONSchema from typescript interface using creation engine",
  "scripts": {
    "script": "cross-env DEBUG=tjscli:* TS_NODE_FILES=true TS_NODE_PROJECT=./tsconfig.prod.json ts-node ./src/tjscli.ts",
    "clean": "rimraf dist artifact",
    "prepub": "npm run build",
    "pub": "npm publish --registry https://registry.npmjs.org --access=public",
    "prepub:dev": "npm run build",
    "pub:dev": "npm publish --registry http://localhost:8901 --force",
    "build:tsc": "tsc --noemit -p tsconfig.json",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "prebuild:dev": "npm run clean",
    "build:dev": "cross-env NODE_ENV=develop webpack --config webpack.config.dev.js",
    "lint": "eslint --ext ts,tsx ./src/*",
    "debug": "cross-env DEBUG=tjscli:* TS_NODE_PROJECT=tsconfig.json TS_NODE_FILES=true node -r ts-node/register -r tsconfig-paths/register --inspect --nolazy src/tjscli.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/tjscli.git"
  },
  "bin": {
    "tjscli": "dist/index.js"
  },
  "private": false,
  "author": "ByungJoon Lee",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/imjuni/tjscli/issues"
  },
  "homepage": "https://github.com/imjuni/tjscli#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/figlet": "^1.5.4",
    "@types/gulp": "^4.0.9",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.4.1",
    "@types/json5": "^2.2.0",
    "@types/luxon": "^2.3.1",
    "@types/node": "^17.0.23",
    "@types/prettier": "^2.4.4",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "docsify-cli": "^4.4.4",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^38.1.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "consola": "^2.15.3",
    "fast-glob": "^3.2.11",
    "fp-ts": "^2.11.9",
    "fuzzy": "^0.1.3",
    "inquirer": "^8.2.2",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "json5": "^2.2.1",
    "luxon": "^2.3.1",
    "my-easy-fp": "^0.11.0",
    "my-node-fp": "^0.2.0",
    "prettier": "^2.6.2",
    "ts-json-schema-generator": "^1.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "typescript-json-schema": "^0.53.0",
    "uuid": "^8.3.2",
    "yargs": "^17.4.0"
  }
}
