{
  "name": "simple-tjscli",
  "version": "1.2.1",
  "description": "Create JSONSchema from typescript interface using creation engine",
  "scripts": {
    "test": "jest --colors --verbose --config ./.configs/jest.config.js",
    "coverage": "jest --coverage --color --fail-fast --verbose --config ./.configs/jest.config.js",
    "clean": "just --config ./.configs/just.config.ts clean",
    "pub:prod": "just --config ./.configs/just.config.ts pub:prod",
    "pub:prod:beta": "just --config ./.configs/just.config.ts pub:prod:beta",
    "pub:dev": "just --config ./.configs/just.config.ts pub:dev",
    "build": "just --config ./.configs/just.config.ts build",
    "rollup:prod": "just --config ./.configs/just.config.ts rollup:prod",
    "lint": "eslint --cache .",
    "lnb": "concurrently \"npm run build\" \"npm run lint\"",
    "dev": "ts-node ./src/cli.ts",
    "debug": "node -r ts-node/register --inspect-brk --nolazy src/cli.ts",
    "dd": "deps-diff md -d prod -d dev --title-list-type '*' --deps-list-type '*' --content-list-type '*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/tjscli.git"
  },
  "private": false,
  "author": "ByungJoon Lee",
  "license": "MIT",
  "main": "dist/cjs/tjscli.js",
  "typings": "dist/esm/tjscli.d.ts",
  "module": "dist/esm/tjscli.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/tjscli.js",
      "import": "./dist/esm/tjscli.js",
      "typings": "./dist/esm/tjscli.d.ts"
    }
  },
  "bin": {
    "tjscli": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/imjuni/tjscli/issues"
  },
  "homepage": "https://github.com/imjuni/tjscli#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@tsconfig/node14": "^1.0.3",
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "^9.0.2",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/jest": "^29.1.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "deps-diff": "^1.0.2",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^5.1.1",
    "jest": "^29.5.0",
    "just-task": "^1.7.0",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "read-pkg": "^5.2.0",
    "rimraf": "^4.3.1",
    "rollup": "^3.18.0",
    "rollup-plugin-ts": "^3.1.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "colors": "^1.4.0",
    "consola": "^2.15.3",
    "fast-glob": "^3.2.12",
    "fast-safe-stringify": "^2.1.1",
    "find-up": "^5.0.0",
    "fuse.js": "^6.6.2",
    "inquirer": "^8.2.4",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "json-schema": "^0.4.0",
    "jsonc-parser": "^3.0.0",
    "mathjs": "^11.6.0",
    "minimist": "^1.2.8",
    "my-easy-fp": "^0.20.2",
    "my-node-fp": "^0.8.1",
    "my-only-either": "^1.1.2",
    "object-traversal": "^1.0.1",
    "ora": "^5.4.1",
    "pino": "^8.11.0",
    "pino-pretty": "^9.4.0",
    "prettier": "^2.8.4",
    "rxjs": "^7.8.0",
    "ts-json-schema-generator": "^1.2.0",
    "ts-morph": "^17.0.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "typescript-json-schema": "^0.55.0",
    "uuid": "^9.0.0",
    "yargs": "^17.7.1"
  }
}