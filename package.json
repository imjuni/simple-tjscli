{
  "name": "simple-tjscli",
  "version": "1.0.0-beta",
  "description": "Create JSONSchema from typescript interface using creation engine",
  "scripts": {
    "dev": "cross-env DEBUG=tjscli:* ts-node ./src/cli.ts",
    "test": "jest --config ./.config/jest.config.js",
    "coverage": "jest --coverage --config ./.config/jest.config.js",
    "clean": "rimraf dist artifact",
    "prepub": "npm run build",
    "pub": "npm publish --registry https://registry.npmjs.org --access=public",
    "prepub:dev": "npm run build",
    "pub:dev": "npm publish --registry http://localhost:8901 --force",
    "build:tsc": "tsc --noemit -p tsconfig.json",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --config ./.config/webpack.config.prod.js",
    "prebuild:dev": "npm run clean",
    "build:dev": "cross-env NODE_ENV=develop webpack --config ./.config/webpack.config.dev.js",
    "lint": "eslint --ext ts,tsx ./src/*",
    "debug": "cross-env DEBUG=tjscli:* node -r ts-node/register --inspect-brk --nolazy src/cli.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/tjscli.git"
  },
  "bin": {
    "tjscli": "dist/index.js"
  },
  "private": false,
  "author": "ByungJoon Lee",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/imjuni/tjscli/issues"
  },
  "homepage": "https://github.com/imjuni/tjscli#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/debug": "^4.1.7",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^8.2.1",
    "@types/inquirer-autocomplete-prompt": "^1.3.4",
    "@types/jest": "^28.1.3",
    "@types/luxon": "^2.3.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.0",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "colors": "^1.4.0",
    "consola": "^2.15.3",
    "fast-glob": "^3.2.11",
    "fast-safe-stringify": "^2.1.1",
    "find-up": "^5.0.0",
    "fuse.js": "^6.6.2",
    "inquirer": "^8.2.4",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "json-schema": "^0.4.0",
    "jsonc-parser": "^3.0.0",
    "luxon": "^2.4.0",
    "mathjs": "^10.6.3",
    "minimist": "^1.2.6",
    "my-easy-fp": "^0.13.0",
    "my-node-fp": "^0.7.0",
    "my-only-either": "^1.1.2",
    "prettier": "^2.7.1",
    "rxjs": "^7.5.5",
    "ts-json-schema-generator": "^1.0.0",
    "ts-morph": "^15.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "typescript-json-schema": "^0.53.1",
    "uuid": "^8.3.2",
    "yargs": "^17.5.1"
  }
}
