  "description": "Create JSONSchema from typescript interface using creation engine",
  "scripts": {
    "test": "jest --colors --verbose --config ./.configs/jest.config.js",
    "coverage": "jest --coverage --color --fail-fast --verbose --config ./.configs/jest.config.js",
    "clean": "just --config ./.configs/just.config.ts clean",
    "pub:prod": "just --config ./.configs/just.config.ts pub:prod",
    "pub:prod:beta": "just --config ./.configs/just.config.ts pub:prod:beta",
    "pub:dev": "just --config ./.configs/just.config.ts pub:dev",
    "build": "just --config ./.configs/just.config.ts build",
    "webpack:prod": "just --config ./.configs/just.config.ts webpack:prod",
    "webpack:dev": "just --config ./.configs/just.config.ts webpack:dev",
    "dts-bundle": "just --config ./.configs/just.config.ts dts-bundle",
    "lint": "just --config ./.configs/just.config.ts lint",
    "dev": "ts-node ./src/cli.ts",
    "debug": "node -r ts-node/register --inspect-brk --nolazy src/cli.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/tjscli.git"
