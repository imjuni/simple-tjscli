{
  engine: 'tsj',

  // current working directory
  cwd: '.',

  // tsconfig path
  project: './tsconfig.json',

  // filename prefix in JSONSchema, default value is JSC_
  prefix: 'JSC_',

  // TSJ generator option
  topRef: false,

  jsDoc: 'extended',

  expose: 'none',

  extraTags: ['dateformatex'],

  // formatContent
  format: "/* eslint-disable @typescript-eslint/naming-convention */\\n\
import { JSONSchema7 } from 'json-schema';\\n\\n\
const JSC_%{{TYPE_NAME}}%: JSONSchema7 = %{{SCHEMA_JSON_CONTENT}}%;\\n\
\\n\
export default JSC_%{{TYPE_NAME}}%;"
}
